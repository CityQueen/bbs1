import java.util.*;
public class Aufgabe_04 {

	public static void main(String[] fghdfhfd) {
		Scanner uff=new Scanner(System.in);
		int o=0, s=0;
		System.out.print("Geben Sie eine Obergrenze an: ");
		o=uff.nextInt();
		for (int i=1;i<o;i=i+2) {
			System.out.println(i+ " " + o);
			s=s+i;
		}
	}

}
