import java.util.*;

public class Aufagbe_5 {

	public static void main(String[] args) {
		System.out.println("Geben Sie eine Zahl ein");
		Scanner eingabe = new Scanner(System.in);
		int x = eingabe.nextInt();
		int y;
		for (y = 1; y <= x; y++) {
			y=y+1;
			System.out.print(y);
			if (y % 2 > 0) {
				
			}

		}
	}
}